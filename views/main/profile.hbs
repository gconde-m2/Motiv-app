 




<div class="container goals-list-content">

    <h2 class="logo">Motiv</h2>
    <div class="list-box">
           <img style="width:200px; text-align:center" src="/images/user.svg" alt="">
        <h1>Bienvenido a tu perfil {{user.username}}</h1>
        <div class="row">
            <div class="col-sm-6 col-md-6 ">
 
                <h4>El usuario se unió en:</h4>
                <ul>
                    <li>    
                        <a href="#">
                            {{userData.createdAt}}
                        </a>            
                    </li>
                    <h4>Última actualización:</h4>
                <ul>
                    <li>    
                        <a href="#">
                            {{userData.updatedAt}}
                        </a>
                    </li>    
                </ul>
                
    </div>
    <div class="col-sm-6 col-md-6 ">
 
                <h4>Lista de goals:</h4>
                <ul>
                    {{#each userData.goal}}
                    <li>    
                        <a href="#">
                            
                            {{name}}
                        </a>            
                    </li>
                 {{/each}}
                </ul>
        </div>

     <a href="/main"> &#8592 back</a>